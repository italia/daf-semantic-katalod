  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/business-event/5">
    <dct:identifier>5</dct:identifier>
    <skos:prefLabel xml:lang="en">Starting/Registering a branch</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/business-event"/>
    <skos:definition xml:lang="it">Questo evento della vita di un'impresa raggruppa servizi pubblici relativi all'avvio di una nuova filiale di un'impresa.</skos:definition>
    <skos:notation>5</skos:notation>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="it">Avvio/registrazione filiale</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/italia/onto/CPSV/BusinessEvent"/>
    <skos:definition xml:lang="en">This business event groups public services related to starting of a new branch of a company.</skos:definition>
    <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
    <skos:prefLabel xml:lang="de">Starten/Registrieren einer Filiale</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/name">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Property"/>
