  <rdf:Description rdf:about="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/business-event/6">
    <skos:prefLabel xml:lang="en">Financing a company</skos:prefLabel>
    <skos:definition xml:lang="en">This business event groups public services related to different types of funding, grants, loans, subsidies that help to finance the business.</skos:definition>
    <skos:notation>6</skos:notation>
    <skos:definition xml:lang="it">Questo evento della vita di un'impresa raggruppa servizi pubblici relativi alla concessione di diversi tipi di finanziamento per un'impresa.</skos:definition>
    <skos:prefLabel xml:lang="it">Finanziamento impresa</skos:prefLabel>
    <skos:inScheme rdf:resource="https://w3id.org/italia/controlled-vocabulary/classifications-for-public-services/life-business-event/business-event"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/italia/onto/CPSV/BusinessEvent"/>
    <dct:identifier>6</dct:identifier>
    <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
    <skos:prefLabel xml:lang="de">Finanzierung eines Unternehmens</skos:prefLabel>
  </rdf:Description>
