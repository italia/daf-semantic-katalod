    <!-- https://w3id.org/italia/onto/AtlasOfPaths/encountersPath -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/AtlasOfPaths/encountersPath">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/AtlasOfPaths/Path"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/AtlasOfPaths/Path"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/italia/onto/Route/hasRoute"/>
            <rdf:Description rdf:about="https://w3id.org/italia/onto/l0/hasMember"/>
            <rdf:Description rdf:about="https://w3id.org/italia/onto/AtlasOfPaths/encountersPathStage"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/l0/hasMember"/>
            </rdf:Description>
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/Route/hasRoute"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">La object property encountersPath permette di definire quali sono i cammini incontrati da un specifico cammino che rappresenta il soggetto delle triple aventi come predicato encountersPath.</rdfs:comment>
        <rdfs:comment xml:lang="en">The encountersPath object property specifies which paths are encountered by a subject path.</rdfs:comment>
        <rdfs:label xml:lang="en">encounters path</rdfs:label>
        <rdfs:label xml:lang="it">incontra cammino</rdfs:label>
    </owl:ObjectProperty>



