                                              dct:language <http://publications.europa.eu/resource/authority/language/ENG> ;
                                              admsapit:hasFormalityLevel admsapit:Formal ;
                                              admsapit:acronym "AtlasOfPaths-AP_IT" ;
                                              admsapit:hasContributor <http://spcdata.digitpa.gov.it/Amministrazione/agid> ;
                                              dct:modified "2018-10-23"^^xsd:date ;
                                              admsapit:hasSemanticAssetDistribution <https://w3id.org/italia/data/distribution/AOPAPIT-RDF-TURTLE> ;
                                              dcat:theme <http://publications.europa.eu/resource/authority/data-theme/EDUC> ;
                                              dct:publisher <https://w3id.org/italia/data/organization/support-unit/cnr-Z6HZEH/stlab> ;
                                              dcat:contactPoint <https://w3id.org/italia/data/contact-point/onto-dgt> ;
                                              rdfs:isDefinedBy <https://w3id.org/italia/onto/AtlasOfPaths> ;
