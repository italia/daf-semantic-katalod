  <rdf:Description rdf:about="https://w3id.org/italia/onto/PARK-aligns">
    <dct:title xml:lang="it">Allineamenti per l'ontologia dei parcheggi</dct:title>
    <dc:creator xml:lang="en">Institute of Cognitive Sciences and Technologies of the Italian Research Council (CNR) - Semantic Technology Laboratory (STLab)</dc:creator>
    <dc:creator xml:lang="en">Italian Digital Transformation Team</dc:creator>
    <dct:title xml:lang="en">Car Park Ontology Alignments</dct:title>
    <dct:description xml:lang="en">These are the alignments to external ontologies widely used and available at the state of the art for the car park ontology</dct:description>
    <owl:imports rdf:resource="https://w3id.org/italia/onto/PARK"/>
    <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-06-25</dc:modified>
