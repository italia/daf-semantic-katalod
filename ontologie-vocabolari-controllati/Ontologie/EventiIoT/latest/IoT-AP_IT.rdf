    <rdfs:subClassOf rdf:nodeID="ub16bL548C30"/>
    <rdfs:subClassOf rdf:nodeID="ub16bL585C30"/>
    <rdfs:label xml:lang="en">Observation</rdfs:label>
    <rdfs:subClassOf rdf:nodeID="ub16bL603C30"/>
