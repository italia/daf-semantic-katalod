    <rdfs:subClassOf rdf:nodeID="ub16bL535C42"/>
    <rdfs:label xml:lang="en">Measurement Quality Value</rdfs:label>
    <rdfs:label xml:lang="it">Valore Qualità di Misurazione</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/iotapit/phenomenonTime">
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:comment xml:lang="it">Questa proprietà è sotto proprietà di atTime ed è utilizzata per modellare il tempo di validità dell'osservazione.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:label xml:lang="en">phenomenon time</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
    <rdfs:comment xml:lang="en">This property is subproperty of atTime and it is used to model the valididty time of an observation.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/tiapit/atTime"/>
    <rdfs:label xml:lang="it">tempo di validità</rdfs:label>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/tiapit/TimeInterval"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/iotapit/observationMadeBySensor">
    <rdfs:comment xml:lang="it">Questa è la proprietù inversa che lega un'osservazione al sensore.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/iotapit/"/>
    <rdfs:label xml:lang="en">observation made by sensor</rdfs:label>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/iotapit/Sensor"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">This is the inverse property of makesObservation and links an Observation to the Sensor.</rdfs:comment>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:label xml:lang="it">osservazione fatta dal sensore</rdfs:label>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/iotapit/Observation"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/iotapit/Actuator">
    <rdfs:comment xml:lang="en">An actuator.</rdfs:comment>
    <rdfs:label xml:lang="it">Attuatore</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Actuator</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Object"/>
    <rdfs:comment xml:lang="it">La classe che rappresenta un attuatore.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub16bL570C30">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
