  <rdf:Description rdf:about="http://dati.gov.it/onto/controlledvocabulary/LE/18">
    <skos:definition xml:lang="en">This life event groups public services related to the access to public cultural sites such as the payment of a ticket or the use of public library</skos:definition>
    <skos:prefLabel xml:lang="it">Accesso luoghi della cultura</skos:prefLabel>
    <clvapit:hasRankOrder>1</clvapit:hasRankOrder>
    <skos:inScheme rdf:resource="http://dati.gov.it/onto/controlledvocabulary/LE"/>
    <skos:definition xml:lang="it">Questo evento della vita raggruppa servizi pubblici relativi all'accesso ai luoghi della cultura pubblici come per esempio il pagamento di un biglietto di entrata, o l'uso di una biblioteca pubblica</skos:definition>
    <dct:identifier>18</dct:identifier>
    <skos:notation>18</skos:notation>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Access to public cultural sites</skos:prefLabel>
  </rdf:Description>
